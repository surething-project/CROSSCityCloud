[req]
default_bits = 4096
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = req_ext

[dn]
C = "PT"
ST = "LX"
O = "CROSSCityServer"
CN = "localhost"

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
IP.1 = ::1
IP.2 = 127.0.0.1
IP.3 = 10.0.2.2
IP.4 = 192.168.1.84
IP.5 = 192.168.1.85
